{
  "name": "nyro",
  "version": "0.1.3",
  "description": "nyro electron app",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "dist": "electron-builder --win -p always"
  },
  "build": {
    "appId": "com.nyrollc.nyro",
    "productName": "Nyro",
    "mac": {
      "target": "dmg",
      "icon": "favicon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "favicon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "trynyro",
        "repo": "nyro-app"
      }
    ]
  },
  "author": "Nyro LLC",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "electron-clipboard-extended": "^1.1.1",
    "electron-log": "^5.2.0",
    "node-powershell": "^5.0.1",
    "nyro": "file:",
    "robotjs": "^0.6.0",
    "update-electron-app": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trynyro/nyro-app"
  }
}
